<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>FreeGame</servlet-name>
        <servlet-class>FreeGame</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FreeGame</servlet-name>
        <url-pattern>/FreeGame</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginCheck</servlet-name>
        <servlet-class>LoginCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginCheck</servlet-name>
        <url-pattern>/LoginCheck</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PlayGame</servlet-name>
        <servlet-class>PlayGame</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PlayGame</servlet-name>
        <url-pattern>/PlayGame</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>

</web-app>